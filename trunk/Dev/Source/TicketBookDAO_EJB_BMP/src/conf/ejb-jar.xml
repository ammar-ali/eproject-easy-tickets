        <entity>
            <display-name>TicketEB</display-name>
            <ejb-name>Ticket</ejb-name>
            <home>ticketbook.ejb.bmp.TicketRemoteHome</home>
            <remote>ticketbook.ejb.bmp.TicketRemote</remote>
            <ejb-class>ticketbook.ejb.bmp.Ticket</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Integer</prim-key-class>
            <reentrant>false</reentrant>
        </entity>
        <entity>
            <display-name>CityEB</display-name>
            <ejb-name>City</ejb-name>
            <home>ticketbook.ejb.bmp.CityRemoteHome</home>
            <remote>ticketbook.ejb.bmp.CityRemote</remote>
            <ejb-class>ticketbook.ejb.bmp.City</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Integer</prim-key-class>
            <reentrant>false</reentrant>
        </entity>
        <entity>
            <display-name>PaymentTypeEB</display-name>
            <ejb-name>PaymentType</ejb-name>
            <home>ticketbook.ejb.bmp.PaymentTypeRemoteHome</home>
